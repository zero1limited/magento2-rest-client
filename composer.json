{
    "name": "zero1/magento2-rest-client",
    "description": "PHP rest client for Magento 2",
    "version": "1.0.10",
    "type": "library",
    "authors": [
        {
            "name": "zero1",
            "email": "support@zero1.co.uk"
        }
    ],
    "keywords": [
        "Magento",
        "Magento 2",
        "Rest Client",
        "API"
    ],
    "autoload": {
        "psr-4": {
            "Magento2\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "require": {
        "dsheiko/magentosearchquerybuilder": "^1.0",
        "guzzlehttp/guzzle": "^6.3|^7"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0"
    }
}
